<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/course_signals/db" VERSION="20120213" COMMENT="XMLDB file for Moodle blocks/course_signals"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_course_signals_config" COMMENT="Config to block instances of course_signals">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="instanceid"/>
        <FIELD NAME="instanceid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Block instance ID" PREVIOUS="id" NEXT="section_type"/>
		<FIELD NAME="section_type" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Section type" PREVIOUS="instanceid" NEXT="parentcourse"/>
        <FIELD NAME="parentcourse" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="Parent course id" PREVIOUS="section_type" NEXT="gradesparent"/>
        <FIELD NAME="gradesparent" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="parentcourse" NEXT="gradesme"/>      
        <FIELD NAME="gradesme" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="gradesparent" NEXT="statparent"/>  
        <FIELD NAME="statparent" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="gradesme" NEXT="statme"/>  
        <FIELD NAME="statme" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="statparent" NEXT="organize_pager"/>   
        <FIELD NAME="organize_pager" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="statme"/>                               
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="instanceid"/>
        <KEY NAME="instanceid" TYPE="foreign" FIELDS="instanceid" REFTABLE="block_instances" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>